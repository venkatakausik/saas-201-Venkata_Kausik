<h2>Create Department</h2>

<%= form_for(@department) do |f| %>
  <div class="form-group">
    <label>Name</label>
    <%= f.text_field :name, class: "form-control", placeholder: "Department Name", required: true %><br/>
    <%= f.submit text: "create", class: "btn btn-primary" %>
  </div>
<% end %>
